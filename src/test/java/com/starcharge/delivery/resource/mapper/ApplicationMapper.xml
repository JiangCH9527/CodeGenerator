<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.starcharge.delivery.mapper.ApplicationMapper">

	<resultMap id="applicationMapper" type="ApplicationDO">
        <result property="id" column="id"/>
        <result property="projectId" column="project_id"/>
        <result property="name" column="name"/>
        <result property="moduleName" column="module_name"/>
        <result property="buildPath" column="build_path"/>
        <result property="healthCheckPath" column="health_check_path"/>
        <result property="deployTime" column="deploy_time"/>
        <result property="emergencyStartTime" column="emergency_start_time"/>
        <result property="emergencyEndTime" column="emergency_end_time"/>
        <result property="serverName" column="server_name"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
        <result property="logbackProfile" column="logback_profile"/>
        <result property="version" column="version"/>
        <result property="isDeleted" column="is_deleted"/>
	</resultMap>


	<sql id="_fields">
		id,project_id,`name`,module_name,build_path,health_check_path,deploy_time,emergency_start_time,emergency_end_time,server_name,create_time,update_time,logback_profile,version,is_deleted
	</sql>

	<sql id="_condition">
		<trim prefix="WHERE" prefixOverrides="AND|OR">
            <if test="id!=null">
				AND id = #{id}
			</if>
            <if test="projectId!=null">
				AND project_id = #{projectId}
			</if>
            <if test="name!=null and ''!=name">
				AND `name` = #{name}
			</if>
            <if test="moduleName!=null and ''!=moduleName">
				AND module_name = #{moduleName}
			</if>
            <if test="buildPath!=null and ''!=buildPath">
				AND build_path = #{buildPath}
			</if>
            <if test="healthCheckPath!=null and ''!=healthCheckPath">
				AND health_check_path = #{healthCheckPath}
			</if>
            <if test="deployTime!=null and ''!=deployTime">
				AND deploy_time = #{deployTime}
			</if>
            <if test="emergencyStartTime!=null and ''!=emergencyStartTime">
				AND emergency_start_time = #{emergencyStartTime}
			</if>
            <if test="emergencyEndTime!=null and ''!=emergencyEndTime">
				AND emergency_end_time = #{emergencyEndTime}
			</if>
            <if test="serverName!=null and ''!=serverName">
				AND server_name = #{serverName}
			</if>
            <if test="createTime!=null and ''!=createTime">
				<![CDATA[AND create_time >= #{dynamicFields_startTime}]]>
			</if>
            <if test="createTime!=null and ''!=createTime">
				<![CDATA[AND create_time < #{dynamicFields_endTime}]]>
			</if>
            <if test="updateTime!=null and ''!=updateTime">
				<![CDATA[AND update_time >= #{dynamicFields_startTime}]]>
			</if>
            <if test="updateTime!=null and ''!=updateTime">
				<![CDATA[AND update_time < #{dynamicFields_endTime}]]>
			</if>
            <if test="logbackProfile!=null and ''!=logbackProfile">
				AND logback_profile = #{logbackProfile}
			</if>
            <if test="version!=null">
				AND version = #{version}
			</if>
            <if test="isDeleted!=null">
				AND is_deleted = #{isDeleted}
			</if>
		</trim>
	</sql>

	<sql id="_primaryKey_condition">
		WHERE id = #{id}
	</sql>

	<sql id="_order">
        <if test="sortByParams!= null and sortByParams.size()>0" >
            ORDER BY
            <foreach collection="sortByParams.keys" index="index" item="sortByParamKey" separator=",">
                <![CDATA[${sortByParamKey} ${sortByParams[sortByParamKey]}]]>
            </foreach>
        </if>
	</sql>

    <insert id="add" parameterType="ApplicationDO" keyProperty="id">
        INSERT INTO application
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id!=null">
				id,
			</if>
            <if test="projectId!=null">
				project_id,
			</if>
            <if test="name!=null and ''!=name">
				`name`,
			</if>
            <if test="moduleName!=null and ''!=moduleName">
				module_name,
			</if>
            <if test="buildPath!=null and ''!=buildPath">
				build_path,
			</if>
            <if test="healthCheckPath!=null and ''!=healthCheckPath">
				health_check_path,
			</if>
            <if test="deployTime!=null and ''!=deployTime">
				deploy_time,
			</if>
            <if test="emergencyStartTime!=null and ''!=emergencyStartTime">
				emergency_start_time,
			</if>
            <if test="emergencyEndTime!=null and ''!=emergencyEndTime">
				emergency_end_time,
			</if>
            <if test="serverName!=null and ''!=serverName">
				server_name,
			</if>
            <if test="createTime!=null and ''!=createTime">
				create_time,
			</if>
            <if test="updateTime!=null and ''!=updateTime">
				update_time,
			</if>
            <if test="logbackProfile!=null and ''!=logbackProfile">
				logback_profile,
			</if>
            <if test="version!=null">
				version,
			</if>
            <if test="isDeleted!=null">
				is_deleted,
			</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id!=null">
				#{id},
			</if>
            <if test="projectId!=null">
				#{projectId},
			</if>
            <if test="name!=null and ''!=name">
				#{name},
			</if>
            <if test="moduleName!=null and ''!=moduleName">
				#{moduleName},
			</if>
            <if test="buildPath!=null and ''!=buildPath">
				#{buildPath},
			</if>
            <if test="healthCheckPath!=null and ''!=healthCheckPath">
				#{healthCheckPath},
			</if>
            <if test="deployTime!=null and ''!=deployTime">
				#{deployTime},
			</if>
            <if test="emergencyStartTime!=null and ''!=emergencyStartTime">
				#{emergencyStartTime},
			</if>
            <if test="emergencyEndTime!=null and ''!=emergencyEndTime">
				#{emergencyEndTime},
			</if>
            <if test="serverName!=null and ''!=serverName">
				#{serverName},
			</if>
            <if test="createTime!=null and ''!=createTime">
				#{createTime},
			</if>
            <if test="updateTime!=null and ''!=updateTime">
				#{updateTime},
			</if>
            <if test="logbackProfile!=null and ''!=logbackProfile">
				#{logbackProfile},
			</if>
            <if test="version!=null">
				#{version},
			</if>
            <if test="isDeleted!=null">
				#{isDeleted},
			</if>
        </trim>
    </insert>

    <update id="update" parameterType="ApplicationDO">
        UPDATE application
        <set>
            <if test="projectId!=null">
				project_id = #{projectId},
			</if>
            <if test="name!=null and ''!=name">
				`name` = #{name},
			</if>
            <if test="moduleName!=null and ''!=moduleName">
				module_name = #{moduleName},
			</if>
            <if test="buildPath!=null and ''!=buildPath">
				build_path = #{buildPath},
			</if>
            <if test="healthCheckPath!=null and ''!=healthCheckPath">
				health_check_path = #{healthCheckPath},
			</if>
            <if test="deployTime!=null and ''!=deployTime">
				deploy_time = #{deployTime},
			</if>
            <if test="emergencyStartTime!=null and ''!=emergencyStartTime">
				emergency_start_time = #{emergencyStartTime},
			</if>
            <if test="emergencyEndTime!=null and ''!=emergencyEndTime">
				emergency_end_time = #{emergencyEndTime},
			</if>
            <if test="serverName!=null and ''!=serverName">
				server_name = #{serverName},
			</if>
            <if test="createTime!=null and ''!=createTime">
				create_time = #{createTime},
			</if>
            <if test="updateTime!=null and ''!=updateTime">
				update_time = #{updateTime},
			</if>
            <if test="logbackProfile!=null and ''!=logbackProfile">
				logback_profile = #{logbackProfile},
			</if>
            <if test="version!=null">
				version = #{version},
			</if>
            <if test="isDeleted!=null">
				is_deleted = #{isDeleted},
			</if>
        </set>
        <include refid="_primaryKey_condition"/>
    </update>

    <select id="get" resultMap="applicationMapper" parameterType="ApplicationDO">
        SELECT
        <include refid="_fields"/>
        FROM application
        <include refid="_primaryKey_condition"/>
    </select>

    <select id="list" resultMap="applicationMapper" parameterType="ApplicationDO">
        SELECT
        <include refid="_fields"/>
        FROM application
        <include refid="_condition"/>
	    <include refid="_order"/>
    </select>

    <select id="listByPage" resultMap="applicationMapper" parameterType="ApplicationDO">
        SELECT
        <include refid="_fields"/>
        FROM application
        <include refid="_condition"/>
        <include refid="_order"/>
        <if test="startRecord != null and pageSize != null and pageSize >0">
            limit #{startRecord},#{pageSize}
        </if>
	    <if test="!(startRecord != null and pageSize != null and pageSize >0)">
		    limit 0,15
	    </if>
    </select>

    <select id="count" resultType="java.lang.Integer" parameterType="ApplicationDO">
        SELECT
        COUNT(*)
        FROM application
        <include refid="_condition"/>
    </select>

    <delete id="delete" parameterType="ApplicationDO">
        DELETE FROM application
        <include refid="_primaryKey_condition"/>
    </delete>

</mapper>